    }
    #zuhu-modal-wrapper .k-header
    {
        border-style: solid;
        border-width: 0 0 1px 1px;
        overflow: hidden;
        padding: 0.5em 0.6em 0.4em;
        text-overflow: ellipsis;
        white-space: nowrap;
    }
    #zuhu-modal-wrapper .k-header .k-link
    {
        display: block;
        line-height: 18px;
        margin: -0.5em -0.6em -0.4em;
        min-height: 18px;
        padding: 0.5em 0.6em 0.4em;
    }
    #zuhu-modal-wrapper .modal
    {
        left: 50%;
        margin: -250px 0 0 -285px;
        width: 570px;
    }
    #zuhu-modal-wrapper .k-grid table tr:hover
    {
        background-image: none, linear-gradient(to bottom, #fff 0px, #efefef 100%);
        cursor: pointer;
        box-shadow: 0 0 1px 1px rgba(255, 255, 255, 0.6) inset, 0 0 2px rgba(0, 0, 0, 0.3);
        border-color: #ccc;
        color: #676767;
    }
    #zuhu-modal-wrapper #zuhu-modal-overlay
    {
        background-color: #dcdcdc;
        display: none;
        height: 100%;
        opacity: 0.75;
        position: absolute;
        top: 0;
        width: 100%;
        left: 0;
    }
    #zuhu-modal-wrapper #zuhu-modal-overlay img
    {
        background-color: #888;
        border-radius: 3px;
        left: 50%;
        margin: -24px;
        position: absolute;
        top: 50%;
    }
    #zuhu-modal-wrapper .modal-body
    {
        position: relative;
    }
    #dvAccountList-wrapper, #dvContactList-wrapper
    {
        display: none;
    }
    
    .totalTable
    {
        border-collapse: collapse;
        font-family: "Trebuchet MS" ,Arial,Helvetica,sans-serif;
        width: 100%;
    }
    .productTable
    {
        border-collapse: collapse;
        font-family: "Trebuchet MS" ,Arial,Helvetica,sans-serif;
        width: 100%;
        font-size: 10px;
    }
    .productTable td, .productTable th
