                                <input class="SettingsName" type="hidden" value="CanadaProductDescription" />
                                <input type="text" id="txtCanadaProductDescription" value="@ViewBag.CanadaProductDescription" class="SettingsValue" style="font-size: larger; width: 300px" />
                                <a role="button" style="vertical-align: super" class="btnSave btn btn-success">
                                    Save
                                </a>
                            </div>
                        </div>
                        <br />
