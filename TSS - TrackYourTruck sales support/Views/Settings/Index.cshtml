                                <input type="text" id="txtShipperCity" value="@ViewBag.ShipperCity" class="SettingsValue" style="font-size: larger; width: 300px" />
                                <a role="button" style="vertical-align: super" class="btnSave btn btn-success">
                                    Save
                                </a>
                            </div>
                        </div>
                        <div class="row-fluid" setting-label="Shipper State" setting-value-type="string">
                            <div class="span2" style="width: 209px">
                                <span style="font-size: large; vertical-align: super; margin-right: 25px">Shipper State: </span>
                            </div>
                            <div class="span10" style="margin-left: 27px;padding-left: 4px;">
