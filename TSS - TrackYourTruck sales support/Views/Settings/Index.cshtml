@{
    string version = System.Configuration.ConfigurationManager.AppSettings["version"];
    ViewBag.Title = "TSS - TrackYourTruck sales support";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="@Url.Content("~/Scripts/Settings/Index.js")?@version" type="text/javascript"></script>
<div class="content-inner">
    <div class="container-fluid">
        <div class="contentHolder">
            <div class="row-fluid">
                <div class="span12">
                    <br>
                    <h2>
                        Settings
                    </h2>
                    <hr style="border-color: yellowgreen;">
                    <div class="contentHolder">
                        <div class="row-fluid" setting-label="Max Hardware Qty" setting-value-type="number">
                            <div class="span2">
                                <span style="font-size: large; vertical-align: super; margin-right: 25px">Max Hardware Qty: </span>
                            </div>
                            <div class="span10">
                                <input class="SettingsName" type="hidden" value="MaxHWQTY" />
                                <input type="text" id="txtMaxQty" value="@ViewBag.MaxHWQTY" class="SettingsValue" style="font-size: larger;" />
                                <a role="button" style="vertical-align: super" class="btnSave btn btn-success">
                                    Save
                                </a>
                            </div>
                        </div>
                        <div class="row-fluid" setting-label="Resend#1" setting-value-type="number">
                            <div class="span2">
                                <span style="font-size: large; vertical-align: super; margin-right: 25px">Quote Resend#1 (hr): </span>
                            </div>
                            <div class="span10">
                                <input class="SettingsName" type="hidden" value="Resend#1" />
                                <input type="text" id="txtResend_1" value="@ViewBag.Resend_1" class="SettingsValue" style="font-size: larger;" />
                                <a role="button" style="vertical-align: super" class="btnSave btn btn-success">
                                    Save
                                </a>
                            </div>
                        </div>
                        <div class="row-fluid" setting-label="Resend#2" setting-value-type="number">
                            <div class="span2">
                                <span style="font-size: large; vertical-align: super; margin-right: 25px">Quote Resend#2 (hr): </span>
                            </div>
                            <div class="span10">
                                <input class="SettingsName" type="hidden" value="Resend#2" />
                                <input type="text" id="txtResend_2" value="@ViewBag.Resend_2" class="SettingsValue" style="font-size: larger;" />
                                <a role="button" style="vertical-align: super" class="btnSave btn btn-success">
                                    Save
                                </a>
                            </div>
                        </div>
                        <div class="row-fluid" setting-label="Resend#3" setting-value-type="number">
                            <div class="span2">
                                <span style="font-size: large; vertical-align: super; margin-right: 25px">Quote Resend#3 (hr): </span>
                            </div>
                            <div class="span10">
                                <input class="SettingsName" type="hidden" value="Resend#3" />
                                <input type="text" id="txtResend_3" value="@ViewBag.Resend_3" class="SettingsValue" style="font-size: larger;" />
                                <a role="button" style="vertical-align: super" class="btnSave btn btn-success">
                                    Save
                                </a>
                            </div>
                        </div>
                        <br />
                        <br />
                        <h3>
                            UPS Settings
                        </h3>
                        <br />
                        <div class="row-fluid" setting-label="Canada Shipper Phone" setting-value-type="string">
                            <div class="span2" style="width: 209px">
                                <span style="font-size: large; vertical-align: super; margin-right: 25px">Canada Shipper Phone: </span>
                            </div>
                            <div class="span10" style="margin-left: 27px;padding-left: 4px;">
                                <input class="SettingsName" type="hidden" value="CanadaShipperPhone" />
                                <input type="text" id="txtCanadaShipperPhone" value="@ViewBag.CanadaShipperPhone" class="SettingsValue" style="font-size: larger;" />
                                <a role="button" style="vertical-align: super" class="btnSave btn btn-success">
                                    Save
                                </a>
                            </div>
                        </div>
